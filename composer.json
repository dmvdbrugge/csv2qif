{
  "name": "dmvdbrugge/csv2qif",
  "description": "Converts given ING CSV to QIF, optionally with advanced matching rules",
  "type": "project",
  "license": "MIT",
  "authors": [
    {
      "name": "Dave van der Brugge",
      "email": "dmvdbrugge@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Config\\": "app/Config",
      "Csv2Qif\\": "app/"
    }
  },
  "require": {
    "php": ">=7.1.0",
    "devvoh/parable": "^1.2",
    "stephenharris/phpqif": "^0.1",
    "dmvdbrugge/dynamic-components": "^0.1",
    "symfony/yaml": "^4.1"
  },
  "suggest": {
    "ext-ui": "Needed if you want to use the GUI (the ui command)"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.13"
  }
}
